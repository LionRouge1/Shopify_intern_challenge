<h1>Items</h1>

<div id="items">
  <% @items.each do |item| %>
    <%= render 'shared/item', obj: item %>
  <% end %>
</div>

<%= link_to "New item", new_item_path, class: 'btn btn-primary' %>

<h2 class="mt-3">Items deleted recently:</h2>

<div data-controller="deleted" data-deleted-target="latest" class="deleted row">
</div>
